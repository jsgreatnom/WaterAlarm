                                    <ImageView
                                        android:id="@+id/fragment_bmi_graph_icon1"
                                        android:layout_width="18dp"
                                        android:layout_height="18dp"
                                        android:background="@drawable/bmi_ico_stick" />
                                </RelativeLayout>

                                <TextView
