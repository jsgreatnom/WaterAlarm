<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/input_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#e9ebe6"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="17.5dp"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="92dp"
                android:layout_height="35dp"
                android:background="@drawable/bmi_table_tit_bg"
                android:gravity="center"
                android:tag="my_info_sex"
                android:text="@string/my_info_sex"
                android:textColor="#000000" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="35dp"
                android:layout_marginLeft="12dp"
                android:layout_weight="1"
                android:background="@drawable/bmi_table_txt_bg"
                android:orientation="horizontal" >

                <RadioGroup
                    android:id="@+id/sexual_radio_group"
                    android:layout_width="wrap_content"
                    android:layout_height="27.5dp"
                    android:orientation="horizontal" >

                    <RadioButton
                        android:id="@+id/radio_woman"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="32dp"
                        android:layout_marginTop="7dp"
                        android:button="@drawable/selector_radio_btn"
                        android:checked="true"
                        android:tag="my_info_woman"
                        android:text="@string/my_info_woman"
                        android:textColor="#404040" />

                    <RadioButton
                        android:id="@+id/radio_man"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="7dp"
                        android:button="@drawable/selector_radio_btn"
                        android:checked="false"
                        android:tag="my_info_man"
                        android:text="@string/my_info_man"
                        android:textColor="#404040" />
                </RadioGroup>
            </LinearLayout>

            <TextView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#696969"
                android:textSize="15dp"
                android:visibility="visible" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="7.5dp"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="92dp"
                android:layout_height="35dp"
                android:background="@drawable/bmi_table_tit_bg"
