                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_weight="1"
                android:background="@drawable/bmi_table_txt_bg"
                android:gravity="center"
                android:inputType="numberDecimal"
                android:maxLength="6" />

            <TextView
                android:id="@+id/text_height_kg"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:text="kg"
                android:textColor="#696969"
                android:textSize="15dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="7.5dp"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="92dp"
                android:layout_height="35dp"
                android:background="@drawable/bmi_table_tit_bg"
                android:gravity="center"
                android:tag="my_info_goal_weight"
                android:text="@string/my_info_goal_weight"
                android:textColor="#000000" />

            <EditText
                android:id="@+id/edit_goal_height"
